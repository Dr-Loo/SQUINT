    <script>
        // Parameter controls
        const controls = {
            period: document.getElementById('periodSlider'),
            duty: document.getElementById('dutySlider'),
            cycles: document.getElementById('cyclesSlider'),
            phase: document.getElementById('phaseSlider'),
            gate: document.getElementById('gateSelect'),
            angle: document.getElementById('angleSlider')
        };

        const values = {
            period: document.getElementById('periodValue'),
            duty: document.getElementById('dutyValue'),
            cycles: document.getElementById('cyclesValue'),
            phase: document.getElementById('phaseValue'),
            angle: document.getElementById('angleValue')
        };

        const previews = {
            period: document.getElementById('previewPeriod'),
            duty: document.getElementById('previewDuty'),
            cycles: document.getElementById('previewCycles'),
            phase: document.getElementById('previewPhase'),
            gate: document.getElementById('previewGate'),
            angle: document.getElementById('previewAngle')
        };

        // Update sliders and preview
        Object.keys(controls).forEach(key => {
            controls[key].addEventListener('input', () => {
                const value = controls[key].value;
                
                if (key === 'gate') {
                    previews[key].textContent = value.toUpperCase();
                    // Show/hide angle control for rotation gates
                    const angleControl = document.getElementById('angleControl');
                    if (value.startsWith('r')) {
                        angleControl.style.display = 'block';
                    } else {
                        angleControl.style.display = 'none';
                    }
                } else if (key === 'duty') {
                    values[key].textContent = value;
                    previews[key].textContent = Math.round(value * 100) + '%';
                } else if (key === 'phase') {
                    values[key].textContent = value;
                    previews[key].textContent = value + '°';
                } else if (key === 'angle') {
                    values[key].textContent = value;
                    previews[key].textContent = value + 'π';
                } else {
                    values[key].textContent = value;
                    previews[key].textContent = key === 'period' ? value + 'ns' : value;
                }
                
                updateTimingAnalysis();
            });
        });

        // Update timing analysis
        function updateTimingAnalysis() {
            const period = parseInt(controls.period.value);
            const duty = parseFloat(controls.duty.value);
            const cycles = parseInt(controls.cycles.value);
            
            const totalDuration = period * cycles;
            const activeTime = totalDuration * duty;
            const idleTime = totalDuration - activeTime;
            const frequency = 1 / period; // GHz
            
            document.getElementById('totalDuration').textContent = totalDuration + 'ns';
            document.getElementById('activeTime').textContent = activeTime.toFixed(1) + 'ns';
            document.getElementById('idleTime').textContent = idleTime.toFixed(1) + 'ns';
            document.getElementById('frequency').textContent = frequency.toFixed(2) + ' GHz';
        }

        // Generate Floquet sequence
        function generateFloquetSequence() {
            const period = parseInt(controls.period.value);
            const duty = parseFloat(controls.duty.value);
            const cycles = parseInt(controls.cycles.value);
            const phaseStep = parseInt(controls.phase.value);
            const gate = controls.gate.value;
            const angle = parseFloat(controls.angle.value);
            
            const onTime = period * duty;
            const offTime = period - onTime;
            
            let sequence = [];
            let quaCode = [];
            
            quaCode.push(`# Floquet ${gate.toUpperCase()} sequence`);
            quaCode.push(`# Period: ${period}ns, Duty: ${(duty * 100).toFixed(0)}%, Cycles: ${cycles}`);
            quaCode.push(`# Phase step: ${phaseStep}° per cycle`);
            quaCode.push('');
            
            for (let i = 0; i < cycles; i++) {
                const phase = (phaseStep * i) % 360;
                const cycleDesc = `Cycle ${i + 1}/${cycles} (phase: ${phase}°)`;
                
                sequence.push(`${cycleDesc}: ${gate.toUpperCase()} gate for ${onTime.toFixed(1)}ns`);
                
                if (gate.startsWith('r')) {
                    quaCode.push(`play('${gate}', q[0], angle=${angle}*π, phase=${phase}deg)  # ${cycleDesc}`);
                } else {
                    quaCode.push(`play('${gate}', q[0], phase=${phase}deg)  # ${cycleDesc}`);
                }
                
                if (offTime > 0) {
                    quaCode.push(`wait(${Math.round(offTime)})  # Idle time`);
                    sequence.push(`${cycleDesc}: Wait for ${offTime.toFixed(1)}ns`);
                }
            }
            
            document.getElementById('sequencePreview').innerHTML = sequence.map(s => `<div class="text-xs">${s}</div>`).join('');
            document.getElementById('quaOutput').innerHTML = `<pre class="mono text-xs text-slate-300">${quaCode.join('\n')}</pre>`;
            
            // Generate waveform visualization
            generateWaveformVisualization(period, duty, cycles, phaseStep);
            
            updateAnalysisResults(period, duty, cycles, phaseStep, gate, angle);
        }

        // Generate waveform visualization
        function generateWaveformVisualization(period, duty, cycles, phaseStep) {
            const timePoints = [];
            const amplitude = [];
            const phase = [];
            
            const samplesPerCycle = 100;
            const totalSamples = cycles * samplesPerCycle;
            
            for (let i = 0; i < totalSamples; i++) {
                const t = (i / totalSamples) * period * cycles;
                const cycleIndex = Math.floor(i / samplesPerCycle);
                const phaseOffset = (phaseStep * cycleIndex * Math.PI) / 180;
                
                timePoints.push(t);
                
                const inCycleTime = (i % samplesPerCycle) / samplesPerCycle;
                const isActive = inCycleTime < duty;
                
                if (isActive) {
                    amplitude.push(Math.sin(2 * Math.PI * inCycleTime / duty + phaseOffset));
                    phase.push(phaseOffset);
                } else {
                    amplitude.push(0);
                    phase.push(phaseOffset);
                }
            }
            
            const trace1 = {
                x: timePoints,
                y: amplitude,
                type: 'scatter',
                mode: 'lines',
                name: 'Drive Amplitude',
                line: { color: '#3b82f6', width: 2 }
            };
            
            const trace2 = {
                x: timePoints,
                y: phase,
                type: 'scatter',
                mode: 'lines',
                name: 'Phase Offset',
                yaxis: 'y2',
                line: { color: '#22c55e', width: 2 }
            };
            
            const layout = {
                title: {
                    text: 'Floquet Drive Waveform',
                    font: { color: '#e2e8f0', size: 16 }
                },
                xaxis: {
                    title: 'Time (ns)',
                    gridcolor: '#374151',
                    color: '#94a3b8'
                },
                yaxis: {
                    title: 'Amplitude',
                    gridcolor: '#374151',
                    color: '#94a3b8'
                },
                yaxis2: {
                    title: 'Phase (rad)',
                    overlaying: 'y',
                    side: 'right',
                    color: '#22c55e'
                },
                plot_bgcolor: 'transparent',
                paper_bgcolor: 'transparent',
                font: { color: '#e2e8f0' },
                legend: {
                    x: 0.02,
                    y: 0.98,
                    bgcolor: 'rgba(30, 41, 59, 0.8)',
                    bordercolor: '#475569',
                    borderwidth: 1
                }
            };
            
            Plotly.newPlot('waveformPlot', [trace1, trace2], layout, {responsive: true});
        }

        // Update analysis results
        function updateAnalysisResults(period, duty, cycles, phaseStep, gate, angle) {
            const onTime = period * duty;
            const offTime = period - onTime;
            const totalTime = period * cycles;
            
            const analysis = [
                `<div><strong>Drive Characteristics:</strong></div>`,
                `<div>• Fundamental frequency: ${(1000/period).toFixed(2)} GHz</div>`,
                `<div>• Total sequence time: ${totalTime}ns</div>`,
                `<div>• Active gate time: ${(onTime * cycles).toFixed(1)}ns (${(duty * 100).toFixed(0)}%)</div>`,
                `<div>• Idle time: ${(offTime * cycles).toFixed(1)}ns (${((1-duty) * 100).toFixed(0)}%)</div>`,
                `<div>• Phase increment: ${phaseStep}° per cycle</div>`,
                `<div>• Total phase sweep: ${(phaseStep * cycles) % 360}°</div>`,
                `<div class="mt-3"><strong>Physical Implementation:</strong></div>`,
                `<div>• Gate type: ${gate.toUpperCase()}${gate.startsWith('r') ? ` (${angle}π)` : ''}</div>`,
                `<div>• Coherence requirement: ~${totalTime * 2}ns (2× safety factor)</div>`,
                `<div>• Control bandwidth: ${(2/duty).toFixed(1)} × fundamental</div>`
            ];
            
            document.getElementById('analysisOutput').innerHTML = analysis.join('');
        }

        // Run Floquet simulation
        function runFloquetSimulation() {
            const period = parseInt(controls.period.value);
            const duty = parseFloat(controls.duty.value);
            const cycles = parseInt(controls.cycles.value);
            const phaseStep = parseInt(controls.phase.value);
            const gate = controls.gate.value;
            
            // Simulate quantum evolution under Floquet drive
            const simulationResults = simulateQuantumEvolution(period, duty, cycles, phaseStep, gate);
            
            // Show results in a modal or dedicated section
            showSimulationResults(simulationResults);
        }

        // Simulate quantum evolution
        function simulateQuantumEvolution(period, duty, cycles, phaseStep, gate) {
            const dt = period / 100; // Time step
            const timePoints = [];
            const population = [];
            const coherence = [];
            
            let pop = 0; // Start in ground state
            let coh = 1.0; // Full coherence
            
            for (let t = 0; t <= period * cycles; t += dt) {
                const cycleIndex = Math.floor(t / period);
                const inCycleTime = (t % period) / period;
                const isActive = inCycleTime < duty;
                
                // Simulate Rabi oscillations during active periods
                if (isActive) {
                    const omega = 2 * Math.PI / (period * duty); // Rabi frequency
                    const phase = (phaseStep * cycleIndex * Math.PI) / 180;
                    pop = Math.sin(omega * inCycleTime * period + phase) ** 2;
                    coh *= 0.999; // Slight decoherence during gates
                } else {
                    coh *= 0.9995; // Decoherence during idle
                }
                
                timePoints.push(t);
                population.push(pop);
                coherence.push(coh);
            }
            
            return {
                timePoints,
                population,
                coherence,
                finalPopulation: pop,
                finalCoherence: coh,
                fidelity: Math.sqrt(coh) // Simplified fidelity
            };
        }

        // Show simulation results
        function showSimulationResults(results) {
            const modal = document.createElement('div');
            modal.className = 'fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50';
            modal.innerHTML = `
                <div class="bg-slate-800 rounded-lg p-6 max-w-2xl max-h-96 overflow-y-auto">
                    <div class="flex justify-between items-center mb-4">
                        <h3 class="text-xl font-semibold text-blue-400">Floquet Simulation Results</h3>
                        <button onclick="this.closest('.fixed').remove()" class="text-slate-400 hover:text-white">
                            <svg class="w-6 h-6" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path>
                            </svg>
                        </button>
                    </div>
                    <div class="space-y-3 text-sm">
                        <div><strong>Final State:</strong></div>
                        <div>• Population: ${results.finalPopulation.toFixed(4)}</div>
                        <div>• Coherence: ${results.finalCoherence.toFixed(4)}</div>
                        <div>• Fidelity: ${results.fidelity.toFixed(4)}</div>
                        <div class="mt-3"><strong>Simulation Complete:</strong></div>
                        <div>• Time points: ${results.timePoints.length}</div>
                        <div>• Total evolution time: ${results.timePoints[results.timePoints.length-1]}ns</div>
                    </div>
                </div>
            `;
            document.body.appendChild(modal);
        }

        // Initialize
        updateTimingAnalysis();
    </script>
</body>
</html>

