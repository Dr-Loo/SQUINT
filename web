    <script>
        // Example SQUINT programs
        const examples = {
            basic: `workspace CalibratedEPR {
    qubits q[4];
    lattice L(2,2) attach q;
    semantic_field Phi: scalar on q;
    defect_field D: defects on q { (0,0), (1,1) };
}

kernel main on CalibratedEPR {
    // Initialize semantic field
    initialize Phi = constant(3.14159);
    
    // Basic quantum operations with overlays
    ctrl rx q[0] angle=π/4 with overlay {
        coherence_len >= 50ns,
        path_len <= 2
    };
    
    ctrl cx q[0], q[1] with overlay {
        coherence_len >= 100ns,
        damping η(Φ=Phi)
    };
    
    measure q[0] -> m0;
    measure q[1] -> m1;
    
    return {m0, m1};
}`,
            
            floquet: `workspace FloquetDemo {
    qubits q[2];
    lattice L(2,1) attach q;
    semantic_field Phi: scalar on q;
}

kernel floquet_sequence on FloquetDemo {
    initialize Phi = constant(2.0);
    
    // Floquet-engineered RX gate
    ctrl rx q[0] angle=π/2 with overlay {
        floquet_period 100ns,
        cycles 20,
        duty 0.3,
        phase_step 15deg,
        coherence_len >= 200ns
    };
    
    // Standard gate for comparison
    ctrl x q[1];
    
    measure q[0] -> m0;
    measure q[1] -> m1;
    
    return {m0, m1, Phi};
}`,
            
            semantic: `workspace SemanticField {
    qubits q[3];
    lattice L(3,1) attach q;
    semantic_field Phi: scalar on q;
    semantic_field Psi: vector on q;
    defect_field D: defects on q;
}

kernel semantic_ops on SemanticField {
    // Initialize fields
    initialize Phi = constant(1.5708);
    initialize Psi = vector(0.1, 0.2, 0.3);
    
    // Nucleate defects
    nucleate D at (0,0), (1,0), (2,0);
    
    // Evolve defect field
    evolve D;
    
    // Quantum operations with semantic awareness
    ctrl h q[0] with overlay {
        damping η(Φ=Phi),
        coherence_len >= 75ns
    };
    
    ctrl cz q[0], q[2] with overlay {
        path_len <= 3,
        braid D,
        coherence_len >= 150ns
    };
    
    // Observe semantic field effects
    observe Phi into obs_Phi with corrections {gain=1.2, offset=0.1};
    
    // Hysteresis analysis
    hysteresis_trace(D, window=5);
    
    measure q[0] -> m0;
    measure q[2] -> m2;
    
    return {obs_Phi, m0, m2};
}`
        };

        // Load example into editor
        function loadExample(type) {
            const editor = document.getElementById('squintCode');
            editor.value = examples[type];
            editor.focus();
        }

        // Analyze SQUINT code
        function analyzeCode() {
            const code = document.getElementById('squintCode').value;
            const output = document.getElementById('analysisOutput');
            
            if (!code.trim()) {
                output.innerHTML = '<div class="text-red-400">Please enter some SQUINT code to analyze</div>';
                return;
            }

            // Simulate analysis (in a real implementation, this would call the Python backend)
            const analysis = simulateAnalysis(code);
            
            output.innerHTML = `
                <div class="space-y-4">
                    <div class="flex items-center space-x-2 text-green-400">
                        <svg class="w-5 h-5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                        </svg>
                        <span class="font-semibold">Parse Successful</span>
                    </div>
                    
                    <div class="grid grid-cols-2 gap-4 text-sm">
                        <div>
                            <span class="text-slate-400">Workspace:</span>
                            <span class="text-blue-300 ml-2">${analysis.workspace}</span>
                        </div>
                        <div>
                            <span class="text-slate-400">Qubits:</span>
                            <span class="text-blue-300 ml-2">${analysis.qubits}</span>
                        </div>
                        <div>
                            <span class="text-slate-400">Lattice:</span>
                            <span class="text-blue-300 ml-2">${analysis.lattice}</span>
                        </div>
                        <div>
                            <span class="text-slate-400">Operations:</span>
                            <span class="text-blue-300 ml-2">${analysis.operations}</span>
                        </div>
                    </div>

                    <div>
                        <h4 class="font-semibold text-slate-300 mb-2">Semantic Fields</h4>
                        <div class="flex flex-wrap gap-2">
                            ${analysis.semanticFields.map(field => 
                                `<span class="px-2 py-1 bg-blue-600/20 text-blue-300 rounded text-xs">${field}</span>`
                            ).join('')}
                        </div>
                    </div>

                    <div>
                        <h4 class="font-semibold text-slate-300 mb-2">Defect Fields</h4>
                        <div class="flex flex-wrap gap-2">
                            ${analysis.defectFields.map(field => 
                                `<span class="px-2 py-1 bg-red-600/20 text-red-300 rounded text-xs">${field}</span>`
                            ).join('')}
                        </div>
                    </div>

                    <div>
                        <h4 class="font-semibold text-slate-300 mb-2">Overlay Constraints Found</h4>
                        <div class="space-y-1">
                            ${analysis.overlays.map(overlay => 
                                `<div class="text-xs mono text-slate-300">${overlay}</div>`
                            ).join('')}
                        </div>
                    </div>

                    <div>
                        <h4 class="font-semibold text-slate-300 mb-2">Compilation Output</h4>
                        <pre class="mono text-xs bg-slate-900/50 p-3 rounded overflow-x-auto text-slate-300">${analysis.quaOutput}</pre>
                    </div>
                </div>
            `;

            // Update circuit visualization
            updateCircuitVisualization(analysis.operationsList);
        }

        // Simulate SQUINT analysis
        function simulateAnalysis(code) {
            // Simple parser simulation - in reality this would use the actual SQUINT parser
            const lines = code.split('\n');
            let workspace = 'Unknown';
            let qubits = 0;
            let lattice = '0x0';
            let semanticFields = [];
            let defectFields = [];
            let operations = 0;
            let overlays = [];
            let operationsList = [];

            lines.forEach(line => {
                line = line.trim();
                
                if (line.startsWith('workspace')) {
                    workspace = line.split(/{/)[0].replace('workspace', '').trim();
                } else if (line.includes('qubits') && line.includes('[')) {
                    const match = line.match(/\[(\d+)\]/);
                    if (match) qubits = parseInt(match[1]);
                } else if (line.includes('lattice') && line.includes('(')) {
                    const match = line.match(/\((\d+),(\d+)\)/);
                    if (match) lattice = `${match[1]}x${match[2]}`;
                } else if (line.includes('semantic_field')) {
                    const match = line.match(/semantic_field\s+(\w+)/);
                    if (match) semanticFields.push(match[1]);
                } else if (line.includes('defect_field')) {
                    const match = line.match(/defect_field\s+(\w+)/);
                    if (match) defectFields.push(match[1]);
                } else if (line.startsWith('ctrl ') || line.startsWith('measure ') || line.startsWith('initialize ')) {
                    operations++;
                    operationsList.push(line);
                } else if (line.includes('with overlay')) {
                    overlays.push(line.trim());
                }
            });

            // Generate QUA-like output
            const quaOutput = `program = QUAProgram()
with program:
    # Workspace: ${workspace}
    # Qubits: ${qubits}, Lattice: ${lattice}
    
${operationsList.map((op, i) => `    # ${op}`).join('\n')}
    
end_program()`;

            return {
                workspace,
                qubits,
                lattice,
                semanticFields,
                defectFields,
                operations,
                overlays,
                operationsList,
                quaOutput
            };
        }

        // Update circuit visualization
        function updateCircuitVisualization(operations) {
            const circuitView = document.getElementById('circuitView');
            
            if (operations.length === 0) {
                circuitView.innerHTML = `
                    <div class="text-slate-400 text-center">
                        <svg class="w-16 h-16 mx-auto mb-4 text-slate-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10"></path>
                        </svg>
                        <p>No quantum operations to visualize</p>
                    </div>
                `;
                return;
            }

            // Create simple circuit visualization
            const qubits = new Set();
            operations.forEach(op => {
                if (op.includes('q[')) {
                    const matches = op.match(/q\[(\d+)\]/g);
                    if (matches) {
                        matches.forEach(match => {
                            const qubitNum = parseInt(match.match(/\d+/)[0]);
                            qubits.add(qubitNum);
                        });
                    }
                }
            });

            const qubitArray = Array.from(qubits).sort((a, b) => a - b);
            const circuitHTML = `
                <div class="w-full">
                    <h4 class="text-lg font-semibold mb-4 text-center">Quantum Circuit</h4>
                    <div class="space-y-4">
                        ${qubitArray.map(qubit => `
                            <div class="flex items-center space-x-4">
                                <div class="w-12 text-right text-sm text-slate-300">q[${qubit}]</div>
                                <div class="flex-1 h-px bg-slate-600 relative">
                                    ${operations.map((op, index) => {
                                        if (op.includes(`q[${qubit}]`)) {
                                            let gateType = '?';
                                            let color = 'bg-gray-500';
                                            
                                            if (op.includes('ctrl rx')) { gateType = 'Rx'; color = 'bg-blue-500'; }
                                            else if (op.includes('ctrl x')) { gateType = 'X'; color = 'bg-red-500'; }
                                            else if (op.includes('ctrl h')) { gateType = 'H'; color = 'bg-purple-500'; }
                                            else if (op.includes('ctrl cz')) { gateType = 'CZ'; color = 'bg-green-500'; }
                                            else if (op.includes('measure')) { gateType = 'M'; color = 'bg-orange-500'; }
                                            
                                            return `<div class="absolute top-1/2 transform -translate-y-1/2 w-8 h-8 ${color} rounded flex items-center justify-center text-xs font-bold" style="left: ${index * 60}px;">${gateType}</div>`;
                                        }
                                        return '';
                                    }).join('')}
                                </div>
                            </div>
                        `).join('')}
                    </div>
                    <div class="mt-6 text-center">
                        <div class="flex justify-center space-x-6 text-xs text-slate-400">
                            <div class="flex items-center space-x-2">
                                <div class="w-4 h-4 bg-blue-500 rounded"></div>
                                <span>Rotation</span>
                            </div>
                            <div class="flex items-center space-x-2">
                                <div class="w-4 h-4 bg-red-500 rounded"></div>
                                <span>Pauli-X</span>
                            </div>
                            <div class="flex items-center space-x-2">
                                <div class="w-4 h-4 bg-purple-500 rounded"></div>
                                <span>Hadamard</span>
                            </div>
                            <div class="flex items-center space-x-2">
                                <div class="w-4 h-4 bg-green-500 rounded"></div>
                                <span>Controlled-Z</span>
                            </div>
                            <div class="flex items-center space-x-2">
                                <div class="w-4 h-4 bg-orange-500 rounded"></div>
                                <span>Measure</span>
                            </div>
                        </div>
                    </div>
                </div>
            `;

            circuitView.innerHTML = circuitHTML;
        }

        // Initialize with basic example
        document.addEventListener('DOMContentLoaded', function() {
            loadExample('basic');
        });
    </script>
</body>
</html>

